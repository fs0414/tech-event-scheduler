{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".output/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "format": {},
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "db:generate": {},
    "db:migrate": {},
    "clean": {
      "cache": false
    }
  }
}
